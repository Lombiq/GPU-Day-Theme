{% Style "http://fonts.googleapis.com/css?family=Montserrat:400,700%7COpen+Sans:300,300i,400,400i,600,600i,700,800" %}
{% StyleRequire "FontAwesome" %}
{% StyleRequire "Bootstrap" %}

{% StyleRequire "Plugins" %}
{% StyleRequire "Site" %}
{% StyleRequire "Index-Event" %}

{% ScriptRequire "jQuery" %}
{% ScriptRequire "Plugins" %}
{% ScriptRequire "Main" %}
<div id="page">
    <div id="main">
        <div id="content-wrapper">
            <div id="content">
                {{ Model.Content | Display }}
            </div>
        </div>
    </div>
    <div id="footer-wrapper">
        <footer>
            {{ Model.Footer | Display }}
        </footer>
    </div>
</div>